    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E74_Group"/>
    <rdfs:comment xml:lang="en">This property is used to indicate the community or group for whom an instance of institutional fact holds. Institutional facts have identity only relative to some group for whom they have a significance as formulated through a chosen symbolic system. </rdfs:comment>
    <rdfs:label xml:lang="en">holds for</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ZP4</skos:notation>
    <rdfs:domain rdf:resource="https://takin.solutions/ontologies/crmaaa/ZE1_Institutional_Fact"/>
  </rdf:Property>
    
